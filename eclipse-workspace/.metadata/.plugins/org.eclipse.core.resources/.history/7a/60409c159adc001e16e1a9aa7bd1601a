package demo;

import cani.Cane;

public class CaneDemo {

	public static void main(String[] args) {
		Cane boby = new Cane("boby","grande","pastore bergamasco", 10);
		Cane fuffy = new Cane("fuffy","medio","setter", 8);
		

		System.out.println(boby.stampaCartellino());
		System.out.println(fuffy.stampaCartellino());

	}

}
