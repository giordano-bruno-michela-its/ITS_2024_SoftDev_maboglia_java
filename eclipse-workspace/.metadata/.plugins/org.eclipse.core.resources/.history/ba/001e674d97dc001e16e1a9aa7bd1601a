package cani;

public class Cane {
	
	String nome=null;
	String taglia;
	String razza;
	int eta;

	public Cane(String nome) {
		this.nome = nome;
	}
	
	
	public String stampaCartellino() {
		
		String output = "Cane: \n" +
						nome +"\n"+
						taglia + "\n"+
						razza + "\n"+
						eta +"\n";
		return output;
		
	}
	
}
